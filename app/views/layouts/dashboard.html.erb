<!DOCTYPE html>
<html>
    <head>
        <title>College Connect</title>
        <%=stylesheet_link_tag "dashboard.css"%>
        <%=stylesheet_link_tag "application.css"%>
        <%= csrf_meta_tags %>
        <%= javascript_include_tag 'application.js' %>
        <%= javascript_include_tag 'handlebars-v4.0.5.js' %>
        <%= javascript_include_tag 'campus_connect/utilities.js' %>
        <%= javascript_include_tag 'campus_connect/dashboard.js' %>
        <%= javascript_include_tag 'campus_connect/connection_dashboard.js' %>
        <%= javascript_include_tag "campus_connect/college/validation.js" %>
    </head>

    <body>
        <%= render 'layouts/header' %>
        <%=yield%>
        <%= render :partial=>"campus_drives/departments_handlebar" %>
        <%= render :partial=>"dashboard/cities_department_handlebar" %>
        <%= render :partial=>"dashboard/cards_handlebar" %>
        <%= render :partial=>"connections_dashboard/cards_handlebar" %>
        <%= render :partial=>"dashboard/company_details_handlebar" %>
    </body>
</html>
